public class NewClass {
    @FunctionalInterface
    interface Function<T,R>{
        R apply(T t); 
    }
    public static void main(String[] args){
        Function<String,Integer> f1 = String::length;
        System.out.println(f1.apply("CanÄ±m Kendim"));
    }
